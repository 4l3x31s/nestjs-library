{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@ruat/lib-ale/(.*)": "<rootDir>/../libs/lib-ale/src/$1",
    "@ruat/lib-ale": "<rootDir>/../libs/lib-ale/src"
  }
}